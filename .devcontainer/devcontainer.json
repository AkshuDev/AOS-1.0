{
  "remoteUser": "vscode",
  "mounts": [
    "source=${env:HOME}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=cached"
  ],
  "forwardPorts": [],
  "remoteEnv": {},
  "extensions": [],
  "postCreateCommand": "ssh-keyscan github.com >> ~/.ssh/known_hosts && git submodule update --init --recursive"
}
